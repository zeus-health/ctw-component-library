{
  "name": "@zus-health/ctw-component-library",
  "version": "0.12.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "typings": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.es.js",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:tailwind": "npm run generate:theme-cjs",
    "generate:theme-cjs": "tsc ./src/styles/tailwind.theme.ts --target es5 --module commonjs && mv ./src/styles/tailwind.theme.js ./tailwind-gen.theme.cjs",
    "build": "npm run generate:theme-cjs && run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "format": "prettier --write .",
    "format-check": "npm run generate:theme-cjs && prettier --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint-fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "storybook": "npm run generate:theme-cjs && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "release": "changeset publish"
  },
  "prettier": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/zeus-health/ctw-component-library.git"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@changesets/cli": "^2.24.3",
    "@kensho-technologies/eslint-config": "^23.1.1",
    "@storybook/addon-actions": "^7.0.0-alpha.31",
    "@storybook/addon-essentials": "^7.0.0-alpha.31",
    "@storybook/addon-interactions": "^7.0.0-alpha.31",
    "@storybook/addon-links": "^7.0.0-alpha.31",
    "@storybook/react": "^7.0.0-alpha.31",
    "@storybook/react-vite": "^7.0.0-alpha.31",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/classnames": "^2.3.1",
    "@types/fhir": "^0.0.35",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.4",
    "list-selectors": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rcs-core": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rename-css-selectors": "^4.1.0",
    "storybook": "^7.0.0-alpha.31",
    "sass": "^1.55.0",
    "storybook": "^7.0.0-alpha.31",
    "tailwindcss": "^3.1.6",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.1"
  },
  "dependencies": {
    "@headlessui-float/react": "^0.9.1",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@storybook/cli": "^7.0.0-alpha.31",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-hook/resize-observer": "^1.2.6",
    "@tanstack/react-query": "^4.3.9",
    "@storybook/cli": "^7.0.0-alpha.31",
    "date-fns": "^2.29.1",
    "fhir-kit-client": "^1.9.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "zod": "^3.19.0",
    "storybook-addon-mock": "^2.4.1"
  }
}
