{
  "name": "@zus-health/ctw-component-library",
  "version": "0.4.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:css": "npm run generate:css -- --watch",
    "generate:css": "npm run generate-theme-cjs && tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind-gen.css",
    "generate-theme-cjs": "tsc ./src/styles/tailwind.theme.ts --target es5 --module commonjs && mv ./src/styles/tailwind.theme.js ./tailwind-gen.theme.cjs",
    "build": "npm run generate:css && run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "format": "prettier --write .",
    "format-check": "npm run generate-theme-cjs && prettier --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint-fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "tsc -b",
    "release": "changeset publish"
  },
  "prettier": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/zeus-health/ctw-component-library.git"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.3",
    "@kensho-technologies/eslint-config": "^23.1.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/classnames": "^2.3.1",
    "@types/fhir": "^0.0.35",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "list-selectors": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rename-css-selectors": "^4.1.0",
    "tailwindcss": "^3.1.6",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.1"
  },
  "dependencies": {
    "@headlessui-float/react": "^0.8.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "date-fns": "^2.29.1",
    "fhir-kit-client": "^1.9.1",
    "lodash": "^4.17.21",
    "zod": "^3.19.0"
  }
}
