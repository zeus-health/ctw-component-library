{
  "name": "@zus-health/ctw-component-library",
  "version": "0.39.1",
  "type": "module",
  "engines": {
    "node": ">=16 <19"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "typings": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.es.js",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:tailwind": "npm run generate:theme-cjs",
    "generate:theme-cjs": "tsc ./src/styles/tailwind.theme.ts --target es5 --module commonjs && mv ./src/styles/tailwind.theme.js ./tailwind-gen.theme.cjs",
    "build": "npm run generate:theme-cjs && run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "format": "prettier --write .",
    "format-check": "npm run generate:theme-cjs && prettier --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint-fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "tsc -b",
    "release": "changeset publish",
    "storybook": "npm run generate:theme-cjs && storybook dev -p 6006",
    "build-storybook": "npm run generate:theme-cjs && storybook build",
    "test-storybook": "test-storybook"
  },
  "prettier": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/zeus-health/ctw-component-library.git"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^16.9.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@changesets/cli": "^2.24.3",
    "@faker-js/faker": "^7.6.0",
    "@kensho-technologies/eslint-config": "^23.1.1",
    "@storybook/addon-docs": "^7.0.0-beta.15",
    "@storybook/addon-essentials": "^7.0.0-beta.15",
    "@storybook/addon-interactions": "^7.0.0-beta.15",
    "@storybook/addon-links": "^7.0.0-beta.15",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^7.0.0-beta.15",
    "@storybook/react-vite": "^7.0.0-beta.15",
    "@storybook/test-runner": "^0.9.0",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^2.4.0",
    "@types/fhir": "^0.0.35",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.1",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.12",
    "classnames": "^2.3.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jsdom": "^20.0.1",
    "msw": "^0.48.2",
    "msw-storybook-addon": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^16.14.0",
    "react-dom": "^16.9.8",
    "rename-css-selectors": "^4.1.0",
    "sass": "^1.55.0",
    "storybook": "^7.0.0-beta.15",
    "tailwindcss": "^3.1.6",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4",
    "vite": "^4.0.3",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-hook/resize-observer": "^1.2.6",
    "@tanstack/react-query": "^4.3.9",
    "@xmldom/xmldom": "^0.8.6",
    "date-fns": "^2.29.1",
    "dompurify": "^2.4.1",
    "fhir-kit-client": "^1.9.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "use-isomorphic-layout-effect": "^1.1.2",
    "uuid": "^9.0.0",
    "xml-beautify": "^1.2.3",
    "xpath": "^0.0.32",
    "zod": "^3.19.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "overrides": {
    "react-json-view": {
      "react": "^16.9.0 || ^17 || ^18",
      "react-dom": "^16.9.0 || ^17 || ^18"
    }
  }
}
