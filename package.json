{
  "name": "@zus-health/ctw-component-library",
  "version": "1.7.8",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16 <19"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "typings": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.es.js",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:tailwind": "npm run generate:theme-cjs",
    "generate:theme-cjs": "tsc ./src/styles/tailwind.theme.ts --target es5 --module commonjs && mv ./src/styles/tailwind.theme.js ./tailwind-gen.theme.cjs",
    "build": "npm run generate:theme-cjs && run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "format": "prettier --write .",
    "format-check": "npm run generate:theme-cjs && prettier --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint-fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "tsc -b",
    "release": "changeset publish",
    "storybook": "npm run generate:theme-cjs && storybook dev -p 6006",
    "build-storybook": "npm run generate:theme-cjs && storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:watch": "test-storybook --watch",
    "prepare": "relative-deps",
    "postinstall": "patch-package"
  },
  "prettier": {
    "printWidth": 100
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zeus-health/ctw-component-library.git"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^16.9.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@changesets/cli": "^2.24.3",
    "@faker-js/faker": "^7.6.0",
    "@kensho-technologies/eslint-config": "^26.1.1",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-docs": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.1.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^2.4.0",
    "@types/fhir": "^0.0.35",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.15.11",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "classnames": "^2.3.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "jsdom": "^20.0.1",
    "msw": "^1.2.1",
    "msw-storybook-addon": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^16.14.0",
    "react-dom": "^16.9.8",
    "relative-deps": "^1.0.7",
    "rename-css-selectors": "^4.1.0",
    "sass": "^1.55.0",
    "storybook": "^7.0.2",
    "tailwindcss": "^3.1.6",
    "tsc-alias": "^1.8.5",
    "typescript": "^4.7.4",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8",
    "vitest-fetch-mock": "^0.2.2"
  },
  "dependencies": {
    "@datadog/browser-logs": "^4.30.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "1.7.3",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-dropdown-menu": "^2.0.2",
    "@react-hook/resize-observer": "^1.2.6",
    "@tanstack/react-query": "^4.3.9",
    "@xmldom/xmldom": "^0.8.6",
    "date-fns": "^2.29.1",
    "dompurify": "^2.4.1",
    "fhir-kit-client": "^1.9.1",
    "i18next": "^22.4.11",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "patch-package": "^6.5.1",
    "react-i18next": "^12.2.0",
    "react-jason": "^1.1.2",
    "use-isomorphic-layout-effect": "^1.1.2",
    "uuid": "^9.0.0",
    "xml-beautify": "^1.2.3",
    "xpath": "^0.0.32",
    "zod": "^3.19.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "overrides": {
    "form-data": "4.0.0"
  },
  "relativeDependencies": {}
}
